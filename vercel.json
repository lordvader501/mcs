{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["db.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },{ "src": "(.*)", "destination": "/index.html" }
  ]
}